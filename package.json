{
  "name": "react-ts-webpack5-boilerplate",
  "version": "1.0.0",
  "description": "react + ts的webpack5模板",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install",
    "dev": "yarn && NODE_ENV=development webpack serve --mode=development --config ./build/webpack.config.js --hot",
    "build": "NODE_ENV=production webpack --mode=production --config ./build/webpack.config.js",
    "lint:eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}",
    "lint": "run-p lint:eslint lint:stylelint",
    "lint:lint-staged": "#lint-staged -c ./.husky/lintstagedrc.js",
    "dll-build:dev": "webpack --mode=development --config ./build/webpack.dll.js",
    "dll-build:prod": "webpack --mode=production --config ./build/webpack.dll.js"
  },
  "author": {
    "name": "Zenquan",
    "url": "https://github.com/Zenquan",
    "email": "jomsoucan@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "react-project",
    "typescript-project",
    "react-typescript",
    "react-ts-quick-starter"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^16.2.3",
    "@types/history": "^4.7.8",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/react-router-dom": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.2.5",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.4",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.1.0",
    "postcss": "^8.3.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^5.3.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.6.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^5.0.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.2",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "history": "^4.10.1",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "normalize.css": "^8.0.1",
    "number-random": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
